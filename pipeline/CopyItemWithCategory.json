{
	"name": "CopyItemWithCategory",
	"properties": {
		"activities": [
			{
				"name": "Copy_yg7",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
